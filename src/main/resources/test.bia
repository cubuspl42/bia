def <A> listId(l : List<A>) {
    return l
}

def <A, B> map(l : List<A>, f : (e : A) : B) {
    def recurse() {
        val fst = first:L(l)
        val tail = drop:L(l, 1)
        val headOut = f(fst)
        val tailOut = map(tail, f)

        return cons:L(headOut, tailOut)
    }

    return if size:L(l) == 0 then listOf() else recurse()
}

def mul2(e : Number) {
    return e % 2 == 0
}

val l1 = listOf(1)

val l2 = listId<Number>(l1)

val l3 = map<Number, Boolean>(l1, mul2)

return l3
